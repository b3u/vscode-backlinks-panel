{
  "name": "vscode-backlinks-panel",
  "displayName": "Backlinks Panel",
  "version": "0.1.0",
  "description": "",
  "main": "./out/extension.js",
  "publisher": "Binyamin Green",
  "categories": [
		"Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/b3u/vscode-backlinks-panel.git"
  },
  "activationEvents": ["onView:backlinks"],
  "contributes": {
    "commands": [
      {
        "command": "backlinks.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      }
    ],
    "viewsWelcome": [
      {
        "view": "backlinks",
        "contents": "No Backlinks found. [Learn more](https://github.com/b3u/vscode-backlinks-panel/#faq)."
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "backlinks.refresh",
          "group": "navigation",
          "when": "view == backlinks"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "backlinks",
          "name": "Backlinks"
        }
      ]
    }
  },
  "engines": {
		"vscode": "^1.34.0"
	},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "start": "npm run watch",
    "watch": "tsc -watch -p ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "notes",
    "wiki links",
    "markdown",
    "backlinks",
    "references"
  ],
  "author": "Binyamin Aron Green",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@types/node": "^13.13.5",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "eslint": "^7.0.0",
    "typescript": "^3.8.3"
  }
}
